<meta charset="utf-8">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<meta name="keywords" content="<%= global.meta.keywords %>">
<meta name="description" content="<%= global.meta.description %>">
<meta name="viewport" content="width=device-width" />
<meta name="format-detection" content="telephone=no">
<link rel="start" href="<%= global.path.root %>" title="Home">
<link rel="shortcut icon" href="<%= global.path.root %>assets/img/common/favicon.ico">

<meta property="og:title" content="<%= global.meta.sitename %>">
<meta property="og:type" content="website">
<meta property="og:url" content="<%= global.path.root %>">
<meta property="og:image" content="<%= global.path.root %>assets/img/common/share.jpg">
<meta property="og:site_name" content="<%= global.meta.sitename %>">
<meta property="og:description" content="<%= global.meta.description %>">

<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:title" content="<%= global.meta.sitename %>">
<meta name="twitter:description" content="<%= global.meta.description %>">
<meta name="twitter:site" content="">
<meta name="twitter:image" content="<%= global.path.root %>assets/img/common/share.jpg">
